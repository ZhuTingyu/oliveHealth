<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_pressed="true">
        <shape >

            <solid android:color="@color/color_transparent_10" >
            </solid>


            <stroke
                android:width="1px"
                android:color="@color/color_transparent_half" >
            </stroke>
        </shape>
    </item>
    <item  >
        <shape >

            <solid android:color="@color/color_transparent" >
            </solid>


            <stroke
                android:width="1px"
                android:color="@color/color_9b9b9b" >
            </stroke>
        </shape>
    </item>
</selector>