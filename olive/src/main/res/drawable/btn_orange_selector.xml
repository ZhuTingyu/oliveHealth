<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>

            <solid android:color="@color/color_e89100_transparent">
            </solid>

            <corners android:radius="0dp">
            </corners>

            <stroke
                    android:width="1px"
                    android:color="@color/color_e89100_transparent">
            </stroke>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>

            <solid android:color="@color/base_color_disable">
            </solid>

            <corners android:radius="0dp">
            </corners>

            <stroke
                    android:width="1px"
                    android:color="@color/base_color_disable">
            </stroke>
        </shape>
    </item>
    <item>
        <shape>

            <solid android:color="@color/color_e89100">
            </solid>

            <corners android:radius="0dp">
            </corners>

            <stroke
                    android:width="1px"
                    android:color="@color/color_e89100">
            </stroke>
        </shape>
    </item>
</selector>